			<em></em>
			
		</p>
		<div id="map"></div>

		<script>
			/*
			the script mus be loaded after the map div is defined,
			otherwise this will not work. Therefore the script is below
			the div.
			The source code below is the example from the leaflet start page.
			*/
			
			var map = L.map('map').setView([-26.548410,27.874684], 12);
			L.tileLayer('http://{s}.tile.osm.org/{z}/{x}/{y}.png', {
			  attribution: '&copy; <a href="http://osm.org/copyright">OpenStreetMap</a> contributors'
			}).addTo(map);
			L.marker([-26.548410,27.874684]).addTo(map)
       .bindPopup('Hey there I am pop up!')
       .openPopup();
       
      
      L.marker([-26.615036, 27.837004]).addTo(map)
       .bindPopup('Hey there I am pop up!')
       .openPopup();
      L.marker([-26.602834, 27.846317]).addTo(map)
       .bindPopup('Hey there I am pop up!')
       .openPopup();
      L.marker([-26.591200, 27.846676]).addTo(map)
       .bindPopup('Hey there I am pop up!')
       .openPopup();
      
      L.marker([-26.583316, 27.834824]).addTo(map)
       .bindPopup('Hey there I am pop up!')
       .openPopup();
      
      L.marker([-26.579985, 27.836062]).addTo(map)
       .bindPopup('Hey there I am pop up!')
       .openPopup();
      L.marker([-26.577651, 27.836356]).addTo(map)
       .bindPopup('Hey there I am pop up!')
       .openPopup();
      L.marker([-26.567325, 27.847820]).addTo(map)
       .bindPopup('Hey there I am pop up!')
       .openPopup();
      L.marker([-26.587789, 27.845915]).addTo(map)
       .bindPopup('Hey there I am pop up!')
       .openPopup();
      L.marker([-26.564655, 27.850816]).addTo(map)
       .bindPopup('Hey there I am pop up!')
       .openPopup();
      L.marker([-26.557710, 27.861529]).addTo(map)
       .bindPopup('Hey there I am pop up!')
       .openPopup();
      L.marker([-26.558251, 27.862253]).addTo(map)
       .bindPopup('Hey there I am pop up!')
       .openPopup();
      L.marker([-26.549546, 27.869401]).addTo(map)
       .bindPopup('Hey there I am pop up!')
       .openPopup();
      L.marker([-26.548287, 27.875171]).addTo(map)
			 .bindPopup('Hey there I am pop up!')
			 .openPopup();
       L.marker([-26.535431, 27.885419]).addTo(map)
        .bindPopup('Hey there I am pop up!')
			 .openPopup();
       L.circle([-26.548410,27.874684], {radius: 8500}).addTo(map);
       
		</script>
